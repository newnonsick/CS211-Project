<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="1330.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.MainPageController">
   <children>
      <BorderPane fx:id="window" prefHeight="800.0" prefWidth="1330.0">
         <left>
            <VBox prefHeight="200.0" prefWidth="150.0" styleClass="navigation-list" BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="121.0" prefWidth="150.0" />
                  <Button fx:id="eventButton" mnemonicParsing="false" onAction="#goToEventList" prefHeight="100.0" prefWidth="218.0" styleClass="navigation-button" text="Event" />
                  <Button fx:id="createEventButton" mnemonicParsing="false" onAction="#goToCreateEvent" prefHeight="100.0" prefWidth="212.0" styleClass="navigation-button" text="Create Event" />
                  <Button fx:id="myEventButton" mnemonicParsing="false" onAction="#goToYourCreatedEvents" prefHeight="100.0" prefWidth="205.0" styleClass="navigation-button" text="My Events" />
                  <Button fx:id="myTeamButton" mnemonicParsing="false" onAction="#handleMyTeamButton" prefHeight="100.0" prefWidth="203.0" styleClass="navigation-button" text="My Teams" />
                  <Button fx:id="userInfoButton" mnemonicParsing="false" onAction="#goToUserInformation" prefHeight="100.0" prefWidth="212.0" styleClass="navigation-button" text="User Info" />
               </children>
            </VBox>
         </left>
         <center>
            <StackPane fx:id="content" prefHeight="800.0" prefWidth="1180.0" stylesheets="@stylesheet.css" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
